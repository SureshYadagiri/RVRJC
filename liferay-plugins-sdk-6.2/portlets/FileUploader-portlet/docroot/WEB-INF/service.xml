<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.company.file" auto-namespace-tables="true">
	<author>SOWJI</author>
	<namespace>fileupload</namespace>

	<entity name="Employee" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="empId" type="long" primary="true"  id-type="increment"/>


		<column name="firstName" type="String" />
		<column name="middleName" type="String" />
		<column name="lastName" type="String" />
		<column name="dateOfBirth" type="Date" />
		<column name="gender" type="String"></column>
		<column name="phno" type="long" />
		
		<column name="email" type="String" />
		
		
		<!-- Order -->

		<order by="asc">
			<order-column name="empId" />
		</order>

		
	</entity>
</service-builder>